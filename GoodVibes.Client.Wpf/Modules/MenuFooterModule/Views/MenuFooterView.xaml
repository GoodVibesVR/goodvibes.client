                Panel.ZIndex="90" 
                Height="25" 
                Width="25" 
                Margin="110,0,0,20" 
                HorizontalAlignment="Left" 
                VerticalAlignment="Bottom" 
                Background="Transparent" 
                BorderThickness="0" 
                Opacity="1" 
                Command="{Binding OpenGithubCommand}">
