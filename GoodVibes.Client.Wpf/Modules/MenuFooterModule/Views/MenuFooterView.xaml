        <Label x:Name="GoodVibesVersion" 
                 HorizontalAlignment="Left" 
                 HorizontalContentAlignment="Center"
                 Margin="0,0,0,0" 
                 Content="GoodVibes 1.0-alpha1.1" 
                 VerticalAlignment="Bottom" 
                 Width="200" 
                 Background="Transparent" 
