                            </Path.Data>
                        </Path>
                    </Canvas>
                </Viewbox>

                <!--Toy List-->
                <ItemsControl ItemsSource="{Binding Path=Toys}"
                          Name="ToysList">
                    <ItemsControl.ItemsPanel>
                        <ItemsPanelTemplate>
                            <StackPanel Orientation="Vertical"
                                    Margin="0, 10, 0, 0"/>
                        </ItemsPanelTemplate>
                    </ItemsControl.ItemsPanel>
                    <ItemsControl.ItemTemplate>
                        <DataTemplate>
                            <StackPanel Orientation="Horizontal"
                            Margin="0, 5, 0, 0">
                                <Button Panel.ZIndex="90" 
                            HorizontalAlignment="Left" 
                            VerticalAlignment="Top" 
                            HorizontalContentAlignment="left"
                            VerticalContentAlignment="Center"
                            Background="Transparent" 
                            BorderThickness="0" 
                            Opacity="1" 
                            Width="170"
                            Command="{Binding NavigateToSettingsCommand}"
                            CommandParameter="">
                                    <StackPanel Orientation="Horizontal"
                                Margin="0, 0, 0, 0">

                                        <Border x:Name="toyIcon" CornerRadius="20" Margin="20 0 0 0">
                                            <Image Source="{Binding ToyIcon}" 
                                       Width="30"
                                       Height="30"/>
                                        </Border>
                                        <Label x:Name="ToyLabel" 
                                   HorizontalAlignment="Left" 
                                   HorizontalContentAlignment="Left"
                                   Margin="0,0,0,0" 
                                   Content="{Binding Path=DisplayName}" 
                                   VerticalAlignment="Center" 
                                   Background="Transparent" 
                                   BorderThickness="0"
                                   Foreground="White" 
                                   FontSize="16" 
                                   FontWeight="Normal" 
                                   FontFamily="Inter"/>
                                    </StackPanel>
                                </Button>
                                <Button Panel.ZIndex="90" 
                                HorizontalAlignment="Left" 
                                VerticalAlignment="Top" 
                                HorizontalContentAlignment="left"
                                VerticalContentAlignment="Center"
                                Background="Transparent" 
                                BorderThickness="0" 
                                Opacity="1" 
                                Width="30"
                                Height="32"
                                Command="{Binding _removeToyCommand}">
                                    <Viewbox xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
                                     Stretch="Uniform"
                                     HorizontalAlignment="Right"
                                     Margin="0, 0, 0, 0">
                                        <Canvas Name="svg19" 
                                            Width="18" 
                                            Height="18">
                                            <Canvas.RenderTransform>
                                                <TranslateTransform X="0" Y="0"/>
                                            </Canvas.RenderTransform>
                                            <Canvas.Resources/>
                                            <Path xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
                                                  Name="path17" 
                                                  StrokeThickness="2" 
                                                  Stroke="#1A000000" 
                                                  StrokeLineJoin="Round" 
                                                  StrokeStartLineCap="Round" 
                                                  StrokeEndLineCap="Round">
                                                <Path.Data>
                                                    <PathGeometry Figures="M11.25 9h-4.5 4.5Zm4.5 0a6.75 6.75 0 1 1-13.5 0 6.75 6.75 0 0 1 13.5 0Z" 
                                                    FillRule="NonZero"/>
                                                </Path.Data>
                                            </Path>
                                        </Canvas>
                                    </Viewbox>
                                </Button>
                            </StackPanel>
                            <DataTemplate.Triggers>
                                <DataTrigger Binding="{ Binding Status}" Value="True">
                                    <Setter Property="Background" TargetName="toyIcon" Value="#CF3084" />
                                </DataTrigger>
                                <DataTrigger Binding="{ Binding Status}" Value="False">
                                    <Setter Property="Background" TargetName="toyIcon" Value="#6A6A6A" />
                                </DataTrigger>
                            </DataTemplate.Triggers>
                        </DataTemplate>
                    </ItemsControl.ItemTemplate>
                </ItemsControl>

                <!--New Toy-->
                <Button Panel.ZIndex="90" 
                    HorizontalAlignment="Left" 
                    VerticalAlignment="Top" 
                    Background="Transparent" 
                    BorderThickness="0" 
                    Opacity="1" 
                    
                    Margin="0, 10, 0, 0"
                    Command="{Binding OpenDiscordCommand}">
                    <Viewbox xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" Stretch="Uniform">
                        <Canvas Name="svg170" Width="200" Height="37">
                            <Canvas.RenderTransform>
                                <TranslateTransform X="0" Y="0"/>
                            </Canvas.RenderTransform>
